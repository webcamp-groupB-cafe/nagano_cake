 <div class="container">
  <div class="row">
    <div class="col-md-1"></div>
    <div class="col-md-8">
      <div>配送先編集</div>
 <%= form_with model: @address, url: address_path(@address), method: :patch do |f| %>
　　　<table>
        <tr>
          <td>郵便番号(ハイフンなし)</td>
          <td><%= f.text_field :postal_code, class:"form-control"  %></td>
          <td></td>
        </tr>
        <tr>
          <td>住所</td>
          <td><%= f.text_field :adress, class:"form-control" %></td>
          <td></td>
        </tr>
        <tr>
          <td>宛名</td>
          <td><%= f.text_field :name, class:"form-control"  %></td>
          <td><%= f.submit "変更内容を保存", class:"btn btn-primary" %></td>
        </tr>
        </table>
        <% end %>

        </div>
        </div>
        </div>

